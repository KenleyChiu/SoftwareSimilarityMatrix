<AnchorPane prefHeight="800.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.InterfaceController">
   <children>
      <TabPane prefHeight="800.0" prefWidth="1300.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Main Menu">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="269.0" prefWidth="760.0">
                     <children>
                        <BorderPane prefHeight="763.0" prefWidth="1300.0">
                           <left>
                              <VBox alignment="TOP_CENTER" prefHeight="675.0" prefWidth="208.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <VBox alignment="CENTER" prefHeight="224.0" prefWidth="253.0" spacing="20.0">
                                       <children>
                                          <Label text="File Types:" />
                                          <RadioButton fx:id="java" mnemonicParsing="false" onAction="#java" selected="true" text="java">
                                             <toggleGroup>
                                                <ToggleGroup fx:id="type" />
                                             </toggleGroup></RadioButton>
                                          <RadioButton fx:id="cpp" mnemonicParsing="false" onAction="#cpp" text="cpp" toggleGroup="$type" />
                                          <RadioButton fx:id="all" mnemonicParsing="false" onAction="#all" text="all" toggleGroup="$type" />
                                       </children>
                                    </VBox>
                                    <VBox alignment="CENTER" prefHeight="194.0" prefWidth="156.0" spacing="20.0">
                                       <children>
                                          <Label text="Comparison:" />
                                          <RadioButton fx:id="byLine" mnemonicParsing="false" onAction="#line" selected="true" text="Line">
                                             <toggleGroup>
                                                <ToggleGroup fx:id="comparison" />
                                             </toggleGroup></RadioButton>
                                          <RadioButton fx:id="byChar" mnemonicParsing="false" onAction="#character" text="Character" toggleGroup="$comparison" />
                                       </children>
                                    </VBox>
                                 </children>
                              </VBox>
                           </left>
                           <top>
                              <HBox alignment="CENTER" prefHeight="88.0" prefWidth="1250.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="Software Similarity Program">
                                       <font>
                                          <Font size="37.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </HBox>
                           </top>
                           <center>
                              <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Pane prefHeight="200.0" prefWidth="200.0">
                                       <children>
                                          <VBox alignment="CENTER" layoutX="90.0" layoutY="471.0" prefHeight="117.0" prefWidth="659.0" spacing="20.0">
                                             <children>
                                                <Label text="Longest Similar String:" />
                                                <Label text="string" />
                                             </children>
                                          </VBox>
                                          <ScrollPane layoutX="28.0" layoutY="140.0" prefHeight="312.0" prefWidth="689.0">
                                             <content>
                                                <AnchorPane fx:id="matrixAnchor" minHeight="0.0" minWidth="0.0" prefHeight="718.0" prefWidth="1982.0" />
                                             </content>
                                          </ScrollPane>
                                          <ListView fx:id="list" layoutX="741.0" layoutY="140.0" prefHeight="314.0" prefWidth="317.0" />
                                          <Label layoutX="808.0" layoutY="100.0" text="Top 5 Similarity Scores" />
                                          <HBox alignment="CENTER" layoutX="28.0" layoutY="21.0" prefHeight="100.0" prefWidth="666.0" spacing="20.0">
                                             <children>
                                                <Label text="Filename:" />
                                                <TextField />
                                                <Button fx:id="compare" mnemonicParsing="false" onAction="#compareFiles" text="Compare" />
                                             </children>
                                          </HBox>
                                          <Button fx:id="exit" layoutX="395.0" layoutY="592.0" mnemonicParsing="false" onAction="#exitProg" text="Exit" />
                                       </children>
                                    </Pane>
                                 </children>
                              </StackPane>
                           </center>
                        </BorderPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Metrics">
            <content>
              <AnchorPane fx:id="metricsAnchor" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox alignment="CENTER" layoutX="332.0" layoutY="51.0" prefHeight="108.0" prefWidth="191.0" spacing="20.0">
                           <children>
                              <Label text="Volume:" />
                              <Label fx:id="vol" text="vol" />
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
